{
  "name": "linje-825a",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server/index.js --outdir=build",
    "dev": "run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server/index.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "prepare": "husky install"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "eslintIgnore": [
    "/api",
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "1.6.4",
    "@heroicons/react": "1.0.6",
    "@prisma/client": "3.15.2",
    "@remix-run/node": "1.6.0",
    "@remix-run/react": "1.6.0",
    "@remix-run/serve": "1.6.0",
    "@remix-run/vercel": "1.6.0",
    "@remix-run/server-runtime": "1.6.0",
    "@vercel/node": "1.15.4",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.1",
    "morgan": "1.10.0",
    "react-dom": "17.0.2",
    "react": "17.0.2",
    "tiny-invariant": "1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "6.3.1",
    "@remix-run/dev": "1.6.0",
    "@remix-run/eslint-config": "1.6.0",
    "@tailwindcss/forms": "0.5.2",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "14.2.1",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.2",
    "@types/eslint": "8.4.3",
    "@types/express": "4.17.13",
    "@types/morgan": "1.9.3",
    "@types/node": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/react": "17.0.45",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.7",
    "binode": "1.0.5",
    "c8": "7.11.3",
    "cross-env": "7.0.3",
    "cypress": "10.1.0",
    "dotenv": "16.0.1",
    "esbuild": "0.14.46",
    "eslint-config-prettier": "8.5.0",
    "eslint": "8.18.0",
    "happy-dom": "5.3.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.0",
    "msw": "0.42.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier-plugin-tailwindcss": "0.1.11",
    "prettier": "2.6.2",
    "prisma": "3.15.2",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.1.3",
    "ts-node": "10.8.1",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.7.4",
    "vite-tsconfig-paths": "3.5.0",
    "vite": "2.9.12",
    "vitest": "0.14.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
